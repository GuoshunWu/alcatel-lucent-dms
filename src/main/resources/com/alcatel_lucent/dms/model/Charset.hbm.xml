<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.alcatel_lucent.dms.model">

	<class name="Charset" table="CHARSET">
		<id column="ID" name="id">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
                <param name="sequence_name">ID_CHARSET</param>
                <param name="initial_value">2000</param>
         	</generator>
		</id>

		<property name="name" type="string" column="NAME" not-null="true" />
		<property name="no" type="integer" column="NO" />
	</class>

</hibernate-mapping>