<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.alcatel_lucent.dms.model" schema="dms" default-cascade="all">

	<class name="Text" table="TEXT">
		<id column="ID" name="id">
			<generator class="native" />
		</id>
		
		<property name="reference" type="string" column="REFERENCE"/>
		<property name="status" type="integer" column="STATUS"/>
		<many-to-one name="context" unique="true" not-null="true"/>
		
	</class>

</hibernate-mapping>